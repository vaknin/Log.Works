{
    "manifest_version": 2,
    "name": "Log.Works",
    "description": "Search inside logs, easily.",
    "version": "1.00",
    "content_scripts": [
      {
        "matches": [
          "*://*.travolutionary.com/Session/D*"
        ],
        "js": ["content.js"]
      }
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html",
      "default_title": "Search the entire session!"
    },
    "icons":{
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "permissions": [
      "tabs", "storage"
    ]
  }